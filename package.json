{
  "name": "linkedin-api-sdk",
  "version": "1.0.0",
  "homepage": "https://github.com/mamankhan99/linkedin-api-sdk",
  "bugs": "https://github.com/mamankhan99/linkedin-api-sdk/issues",
  "repository": "https://github.com/mamankhan99/linkedin-api-sdk",
  "author": "Maman <mamankhan99@gmail.com>",
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn run clean && yarn run build:esm && yarn run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "yarn run build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "2.8.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "^4.9.5"
  }
}
